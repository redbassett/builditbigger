<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_laugh"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.udacity.gradle.laughactivity.LaughActivity">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/joke_goes_here"
        tools:text="What do you do when you see a space man? You park your car, man!"
        tools:layout_editor_absoluteX="0dp"
        app:layout_constraintTop_toTopOf="@+id/activity_laugh"
        app:layout_constraintBottom_toBottomOf="@+id/activity_laugh"
        app:layout_constraintStart_toStartOf="@id/activity_laugh"
        app:layout_constraintEnd_toEndOf="@id/activity_laugh"
        android:padding="10dp"/>
</android.support.constraint.ConstraintLayout>
